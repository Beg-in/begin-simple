{
  "name": "replace-me",
  "version": "0.0.1",
  "description": "replace-me",
  "main": "index.js",
  "author": "",
  "license": "",
  "scripts": {
    
    
    "clean": "rimraf docs",
    "autoprefixer": "postcss -u autoprefixer -r docs/styles/*",
    "css": "rsync -r --include=\"*/\" --include=\"*.css\" --exclude=\"*\" src/styles docs",
    "sass": "node-sass --indented-syntax true --output-style compressed -o docs/styles src/styles",
    "uglify": "mkdirp docs/scripts -p && uglifyjs src/scripts/*.js -m -o docs/js/app.js && uglifyjs src/scripts/*.js -m -c -o docs/scripts/app.min.js",
    "imagemin": "imagemin src/images/**/*.{png,jpg,jpeg,gif,ico} -o docs/images",
    "svgmin": "svgo -f src/images/**/*.svg -o docs/images/svgmin",
    "serve": "browser-sync start --server --files \"docs/css/*.css, docs/scripts/*.js, **/*.html, !node_modules/**/*.html\"",
    
    
    "build:css": "run-s sass css",
    "build:js": "run-s uglify",
    "build:images": "run-s imagemin svgmin",
    "build": "run-s build:*",
    
    
    "watch:css": "onchange \"src/styles\" -- run-s build:css",
    "watch:js": "onchange \"src/scripts\" -- run-s build:js",
    "watch:images": "onchange \"src/images\" -- run-s build:images",
    "watch": "run-p serve watch:*"
  },
  "devDependencies": {
    "del": "3.0.0",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "4.0.0",
    "gulp-clean-css": "3.9.0",
    "gulp-htmlmin": "3.0.0",
    "gulp-minify": "1.0.0",
    "gulp-pug": "3.3.0",
    "gulp-sass": "3.1.0",
    "gulp-webserver": "0.9.1",
    
    
    "autoprefixer": "^6.3.6",
    "browser-sync": "^2.12.8",
    "imagemin-cli": "^3.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^2.1.1",
    "onchange": "^2.4.0",
    "postcss-cli": "^2.5.2",
    "rimraf": "^2.5.4",
    "svg-sprite-generator": "0.0.7",
    "svgo": "^0.6.6",
    "uglify-js": "^2.6.2"
  },
  "dependencies": {
    "frow": "2.0.1",
    "letsgo": "0.2.3"
  }
}
